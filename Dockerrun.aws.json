{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "angelahjk25/agendaally-backend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "DB_URL",
          "value": "jdbc:mysql://db:3306/webapp_db"
        },
        {
          "name": "DB_USER",
          "value": "Angela"
        },
        {
          "name": "DB_PASSWORD",
          "value": "Angela123"
        }
      ]
    },
    {
      "name": "frontend",
      "image": "angelahjk25/agendaally-frontend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    },
    {
      "name": "db",
      "image": "mysql:8.0",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 3306,
          "containerPort": 3306
        }
      ],
      "environment": [
        {
          "name": "MYSQL_DATABASE",
          "value": "webapp_db"
        },
        {
          "name": "MYSQL_USER",
          "value": "Angela"
        },
        {
          "name": "MYSQL_PASSWORD",
          "value": "Angela123"
        }
      ]
    }
  ]
}
