{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "angelahjk25/agendaally-backend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ]
    },
    {
      "name": "frontend",
      "image": "angelahjk25/agendaally-frontend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    },
    {
      "name": "db",
      "image": "mysql:8.0",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 3306,
          "containerPort": 3306
        }
      ]
    }
  ]
}
